{
  "name": "sbw-portfolio",
  "version": "1.0.0",
  "description": "React + Vite App Deployed to Cloud Run",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "vite",
    "dev": "vite build --mode development --watch",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fix": "eslint --fix --ext js,jsx,ts,tsx",
    "build-image": "gcloud builds submit --pack image=europe-west2-docker.pkg.dev/$GOOGLE_CLOUD_PROJECT/samples/microservice-template:manual",
    "deploy": "gcloud run deploy microservice-template --image europe-west2-docker.pkg.dev/$GOOGLE_CLOUD_PROJECT/samples/microservice-template:manual --region europe-west2",
    "test": "mocha test/app.test.js --check-leaks",
    "system-test": "mocha test/system.test.js --timeout=180000",
    "build": "vite build",
    "preview": "vite preview"
  },
  "author": "Google LLC",
  "license": "Apache-2.0",
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@react-three/drei": "^9.82.0",
    "@react-three/fiber": "^8.14.1",
    "daisyui": "^3.6.5",
    "deep-email-validator": "^0.1.21",
    "env-var": "^7.4.1",
    "express": "^4.17.1",
    "framer-motion": "^10.16.3",
    "gcp-metadata": "^6.0.0",
    "google-auth-library": "^9.0.0",
    "maath": "^0.7.0",
    "pino": "^8.15.1",
    "pino-http": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-social-icons": "^6.3.0",
    "react-spinners": "^0.13.8",
    "react-tilt": "^1.0.2",
    "react-vertical-timeline-component": "^3.6.0",
    "three": "^0.156.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-vertical-timeline-component": "^3.3.3",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "got": "^13.0.0",
    "mocha": "^10.0.0",
    "nodemon": "^3.0.0",
    "postcss": "^8.4.29",
    "supertest": "^6.1.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.5"
  }
}
